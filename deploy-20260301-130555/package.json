    "fabric": "^7.2.0",
    "framer-motion": "^12.34.0",
    "immer": "^11.1.4",
    "ioredis": "^5.9.2",
    "jose": "^6.1.3",
    "jsonwebtoken": "^9.0.3",
    "jspdf": "^4.2.0",
    "jspdf-autotable": "^5.0.7",
    "lucide-react": "^0.563.0",
    "mammoth": "^1.11.0",
    "mathjax": "^4.1.1",
    "nanoid": "^5.1.6",
    "next": "^16.1.6",
    "next-auth": "^4.24.13",
    "next-intl": "^4.8.3",
    "ollama": "^0.6.3",
    "openai": "^6.19.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^2.4.5",
    "pdfjs-dist": "^4.2.67",
    "pg": "^8.13.0",
    "plotly.js-dist": "^3.4.0",
    "prisma": "^5.22.0",
    "react": "^18",
    "react-day-picker": "^9.13.2",
    "react-dom": "^18",
    "react-resizable-panels": "^4.6.4",
    "recharts": "^3.7.0",
    "redis": "^5.11.0",
    "server-only": "^0.0.1",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "sonner": "^2.0.7",
    "swagger-ui-react": "^5.31.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tesseract.js": "^7.0.0",
    "three": "^0.182.0",
    "tldraw": "^4.4.0",
    "unpdf": "^1.4.0",
    "xlsx": "^0.18.5",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.29",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@playwright/test": "^1.49.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.19.33",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "@types/swagger-ui-react": "^5.18.0",
    "dotenv": "^17.3.1",
    "drizzle-kit": "^0.30.0",
    "eslint": "^9",
    "eslint-config-next": "^16.1.6",
    "eslint-config-prettier": "^10",
    "jsdom": "^25.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  },
  "overrides": {
    "react": "^18",
    "react-dom": "^18"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}