{
  "name": "tutorme-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx server.ts",
    "dev:next": "next dev --port 3003",
    "dev:all": "bash scripts/dev.sh",
    "build": "npm run build:sw && next build",
    "build:sw": "node scripts/build-sw.js",
    "start": "next start",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:a11y": "playwright test src/__tests__/accessibility",
    "test:load": "k6 run --vus 10 --duration 15s scripts/load/concurrent-users.js",
    "test:load:ai": "k6 run --vus 5 --duration 15s scripts/load/ai-stress.js",
    "test:load:ws": "k6 run --vus 5 --duration 15s scripts/load/websocket.js",
    "lint": "eslint .",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "security:check": "npm audit --audit-level=high",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "db:setup": "bash scripts/setup-db.sh",
    "db:migrate": "drizzle-kit push",
    "db:migrate:deploy": "tsx src/lib/db/drizzle-migrate.ts",
    "studio": "bash scripts/studio.sh",
    "test:register": "bash scripts/test-register.sh",
    "db:studio": "bash scripts/studio.sh",
    "db:reset": "bash scripts/reset.sh",
    "db:generate": "drizzle-kit generate",
    "prisma:generate": "prisma generate",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:pull": "drizzle-kit pull",
    "drizzle:studio": "drizzle-kit studio",
    "drizzle:push": "drizzle-kit push",
    "drizzle:migrate": "tsx src/lib/db/drizzle-migrate.ts",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:admin": "tsx src/scripts/seed-admin.ts",
    "data-retention": "tsx scripts/data-retention.ts",
    "initialize": "bash scripts/initialize.sh",
    "start:demo": "bash tutorme-start.sh",
    "initialize:win": "scripts\\initialize.bat",
    "db:check": "bash scripts/check-db.sh",
    "db:check:win": "scripts\\check-db.bat",
    "copy-pdf-worker": "node scripts/copy-pdf-worker.js",
    "postinstall": "node scripts/copy-pdf-worker.js 2>/dev/null || true; echo 'Run prisma generate manually'",
    "ollama:pull": "docker exec tutorme-ollama ollama pull llama3.1",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.994.0",
    "@aws-sdk/s3-request-presigner": "^3.994.0",
    "@daily-co/daily-js": "^0.87.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
